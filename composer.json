{
    "name": "tipoff/booking-calendar",
    "description": "Booking calendar tool for Nova.",
    "keywords": [
        "tipoff",
        "waivers"
    ],
    "homepage": "https://github.com/tipoff/booking-calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "tipoff/support": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\BookingCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\BookingCalendar\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
