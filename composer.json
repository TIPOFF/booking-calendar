{
    "name": "tipoff/booking-calendar",
    "description": "Booking calendar tool for Nova.",
    "keywords": [
        "tipoff"
    ],
    "homepage": "https://github.com/tipoff/booking-calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "tipoff/authorization": "^2.5.5",
        "tipoff/support": "^1.6.6"
    },
    "require-dev": {
        "tipoff/test-support": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\BookingCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\BookingCalendar\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
